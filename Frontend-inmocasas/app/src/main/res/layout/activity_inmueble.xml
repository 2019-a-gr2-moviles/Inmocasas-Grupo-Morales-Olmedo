            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.387"/>
    <Button
            android:layout_width="48dp"
            android:layout_height="76dp"
            android:id="@+id/btn_der" android:layout_marginTop="136dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="296dp" android:layout_marginStart="296dp" android:background="@drawable/arrowr"/>
    <Button
            android:layout_width="48dp"
            android:layout_height="76dp"
            android:id="@+id/btn_izq" android:layout_marginTop="136dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:background="@drawable/arrow" android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <TextView
            android:text="Sector:"
            android:layout_width="75dp"
            android:layout_height="23dp"
            android:id="@+id/textView3" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/img_inmueble" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="44dp" android:layout_marginStart="44dp" android:textStyle="bold"
            android:textSize="18sp"/>
    <TextView
            android:text="Calle Principal:"
            android:layout_width="172dp"
            android:layout_height="22dp"
            android:id="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/txt_sector"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:textStyle="bold" android:textSize="18sp"/>
    <TextView
            android:text="Calle Secundaria:"
            android:layout_width="152dp"
            android:layout_height="23dp"
            android:id="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/txt_calle_p" android:layout_marginTop="4dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:textStyle="bold" android:textSize="18sp"/>
    <TextView
            android:text="Numero de Baños:"
            android:layout_width="156dp"
            android:layout_height="23dp"
            android:id="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView17" android:textStyle="bold" android:textSize="18sp"/>
    <TextView
            android:text="Numero de Parqueaderos:"
            android:layout_width="227dp"
            android:layout_height="32dp"
            android:id="@+id/textView19"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="40dp"
            android:layout_marginStart="40dp" android:textStyle="bold" android:textSize="18sp"/>
    <TextView
            android:text="Ciudad:"
            android:layout_width="173dp"
            android:layout_height="21dp"
            android:id="@+id/textView21"
            android:layout_marginTop="88dp" app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:textStyle="bold" android:textSize="18sp"/>
    <TextView
            android:text="Año:"
            android:layout_width="170dp"
            android:layout_height="23dp"
            android:id="@+id/textView23"
            android:layout_marginTop="56dp" app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:textStyle="bold" android:textSize="18sp"/>
    <TextView
            android:text="Numero de Habitaciones:"
            android:layout_width="220dp"
            android:layout_height="23dp"
            android:id="@+id/textView17"
            app:layout_constraintTop_toBottomOf="@+id/textView15"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:layout_marginTop="16dp" android:textStyle="bold"
            android:textSize="18sp"/>
    <TextView
            android:text="Numero de Pisos:"
            android:layout_width="162dp"
            android:layout_height="24dp"
            android:id="@+id/textView15"
            app:layout_constraintTop_toBottomOf="@+id/txt_calle_s"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:textStyle="bold" android:textSize="18sp"/>
    <TextView
            android:text="TextView"
            android:layout_width="292dp"
            android:layout_height="23dp"
            android:id="@+id/txt_sector"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="44dp" android:layout_marginStart="44dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="296dp"
            android:layout_height="27dp"
            android:id="@+id/txt_calle_p"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="102dp"
            android:layout_height="19dp"
            android:id="@+id/txt_numero_pisos"
            app:layout_constraintTop_toBottomOf="@+id/txt_calle_s"
            app:layout_constraintStart_toEndOf="@+id/textView15" android:layout_marginLeft="4dp"
            android:layout_marginStart="4dp" android:layout_marginTop="2dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="102dp"
            android:layout_height="19dp"
            android:id="@+id/txt_numero_habitaciones"
            app:layout_constraintTop_toBottomOf="@+id/txt_numero_pisos"
            app:layout_constraintStart_toEndOf="@+id/textView17"
            android:layout_marginTop="23dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="101dp"
            android:layout_height="21dp"
            android:id="@+id/txt_numero_banios"
            app:layout_constraintStart_toEndOf="@+id/textView11" android:layout_marginLeft="19dp"
            android:layout_marginStart="19dp" android:layout_marginTop="63dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_numero_pisos"/>
    <TextView
            android:text="TextView"
            android:layout_width="102dp"
            android:layout_height="19dp"
            android:id="@+id/txt_ciudad"
            android:layout_marginTop="168dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_numero_pisos"
            app:layout_constraintStart_toEndOf="@+id/textView21"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="102dp"
            android:layout_height="19dp"
            android:id="@+id/txt_anio"
            android:layout_marginTop="136dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_numero_pisos"
            app:layout_constraintStart_toEndOf="@+id/textView23"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="102dp"
            android:layout_height="19dp"
            android:id="@+id/txt_numero_parqueaderos"
            android:layout_marginTop="102dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_numero_pisos"
            app:layout_constraintStart_toEndOf="@+id/textView19"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="296dp"
            android:layout_height="22dp"
            android:id="@+id/txt_calle_s"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:layout_marginTop="4dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>