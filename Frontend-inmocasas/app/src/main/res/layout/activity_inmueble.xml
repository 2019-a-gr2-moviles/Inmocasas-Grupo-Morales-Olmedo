<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".NavegacionUsuario.InmuebleActivity">

    <ImageView
            android:layout_width="251dp"
            android:layout_height="129dp" tools:srcCompat="@tools:sample/avatars[0]"
            android:id="@+id/img_inmueble" android:layout_marginTop="4dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.504"/>
    <Button
            android:layout_width="48dp"
            android:layout_height="76dp"
            android:id="@+id/btn_der" android:layout_marginTop="32dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="310dp" android:layout_marginStart="310dp" android:background="@drawable/arrowr"/>
    <Button
            android:layout_width="48dp"
            android:layout_height="76dp"
            android:id="@+id/btn_izq" android:layout_marginTop="32dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:background="@drawable/arrow"/>
    <TextView
            android:text="Sector:"
            android:layout_width="75dp"
            android:layout_height="23dp"
            android:id="@+id/textView3" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/img_inmueble" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="44dp" android:layout_marginStart="44dp" android:textStyle="bold"/>
    <TextView
            android:text="Calle Principal:"
            android:layout_width="115dp"
            android:layout_height="22dp"
            android:id="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/txt_sector"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:textStyle="bold"/>
    <TextView
            android:text="Calle Secundaria:"
            android:layout_width="115dp"
            android:layout_height="22dp"
            android:id="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/txt_calle_p" android:layout_marginTop="4dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:textStyle="bold"/>
    <TextView
            android:text="Numero de Baños:"
            android:layout_width="151dp"
            android:layout_height="18dp"
            android:id="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/textView17" android:textStyle="bold"/>
    <TextView
            android:text="Numero de Parqueaderos:"
            android:layout_width="171dp"
            android:layout_height="16dp"
            android:id="@+id/textView19"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="40dp"
            android:layout_marginStart="40dp" android:textStyle="bold"/>
    <TextView
            android:text="Ciudad:"
            android:layout_width="171dp"
            android:layout_height="16dp"
            android:id="@+id/textView21"
            android:layout_marginTop="88dp" app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:textStyle="bold"/>
    <TextView
            android:text="Año:"
            android:layout_width="171dp"
            android:layout_height="16dp"
            android:id="@+id/textView23"
            android:layout_marginTop="56dp" app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:textStyle="bold"/>
    <TextView
            android:text="Numero de Habitaciones:"
            android:layout_width="168dp"
            android:layout_height="18dp"
            android:id="@+id/textView17"
            app:layout_constraintTop_toBottomOf="@+id/textView15"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:layout_marginTop="16dp" android:textStyle="bold"/>
    <TextView
            android:text="Numero de Pisos:"
            android:layout_width="120dp"
            android:layout_height="21dp"
            android:id="@+id/textView15"
            app:layout_constraintTop_toBottomOf="@+id/txt_calle_s"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:textStyle="bold"/>
    <TextView
            android:text="TextView"
            android:layout_width="292dp"
            android:layout_height="23dp"
            android:id="@+id/txt_sector"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="44dp" android:layout_marginStart="44dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="296dp"
            android:layout_height="27dp"
            android:id="@+id/txt_calle_p"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="102dp"
            android:layout_height="19dp"
            android:id="@+id/txt_numero_pisos"
            app:layout_constraintTop_toBottomOf="@+id/txt_calle_s"
            app:layout_constraintStart_toEndOf="@+id/textView15" android:layout_marginLeft="48dp"
            android:layout_marginStart="48dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="102dp"
            android:layout_height="19dp"
            android:id="@+id/txt_numero_habitaciones"
            app:layout_constraintTop_toBottomOf="@+id/txt_numero_pisos"
            app:layout_constraintStart_toEndOf="@+id/textView17"
            android:layout_marginTop="18dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="101dp"
            android:layout_height="21dp"
            android:id="@+id/txt_numero_banios"
            app:layout_constraintStart_toEndOf="@+id/textView11" android:layout_marginLeft="19dp"
            android:layout_marginStart="19dp" android:layout_marginTop="52dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_numero_pisos"/>
    <TextView
            android:text="TextView"
            android:layout_width="102dp"
            android:layout_height="19dp"
            android:id="@+id/txt_ciudad"
            android:layout_marginTop="156dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_numero_pisos"
            app:layout_constraintStart_toEndOf="@+id/textView21"
    />
    <TextView
            android:text="TextView"
            android:layout_width="102dp"
            android:layout_height="19dp"
            android:id="@+id/txt_anio"
            android:layout_marginTop="125dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_numero_pisos"
            app:layout_constraintStart_toEndOf="@+id/textView23"
    />
    <TextView
            android:text="TextView"
            android:layout_width="102dp"
            android:layout_height="19dp"
            android:id="@+id/txt_numero_parqueaderos"
            android:layout_marginTop="85dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_numero_pisos"
            app:layout_constraintStart_toEndOf="@+id/textView19"
            android:layout_marginLeft="5dp" android:layout_marginStart="5dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="296dp"
            android:layout_height="22dp"
            android:id="@+id/txt_calle_s"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" android:layout_marginTop="4dp"/>
</android.support.constraint.ConstraintLayout>